    },
    "scripts": {
        "dev:server": "DEBUG=vuexpressbp:* CONTEXT=unsecure nodemon --config nodemon.json -e js ./bin/www",
        "dev:server:secure": "CONTEXT=secure DEBUG=vuexpressbp:* nodemon --config nodemon.json -e js ./bin/www",
        "dev:client": "NODE_ENV=development webpack --mode=development",
        "prod:client": "NODE_ENV=production webpack --mode=production",
        "prod:server": "DEBUG=vuexpressbp:* CONTEXT=unsecure NODE_ENV=production node ./bin/www",
        "prodbin:server": "DEBUG=vuexpressbp:* CONTEXT=unsecure NODE_ENV=production node ./exe",
        "dev": "DEBUG=vuexpressbp:* CONTEXT=unsecure nodemon ./bin/www --config nodemon.json -e js & webpack --mode=development",
        "dev:secure": "DEBUG=vuexpressbp:* CONTEXT=secure nodemon ./bin/www --config nodemon.json -e js & webpack --mode=development",
        "prestart": "webpack --mode=production",
        "start": "node ./bin/www",
        "test": "mocha --timeout 10000",
        "test:browser": "mocha --timeout 10000  --reporter mocha-simple-html-reporter/index.js --reporter-options output=./test/index.html && open-cli  ./test/index.html || xdg-open ./test/index.html"
    },
    "pkg": {
        "assets": [
            "views/**/*",
            "public/**/*",
            "locales/**/*",
            "key/**/*",
            "config/**/*",
            "app.js"
